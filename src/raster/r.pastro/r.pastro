  g.message -e "ERRORE: carta dei sentieri non specificata"
  exit 1
fi

if [ -z "$GIS_OPT_SHELTERS" ] ; then
	g.message " "  
else 
	shelters="$GIS_OPT_SHELTERS"
fi

export a
export b
export c
export d 
export s_f
export tmp_frane
export tmp_no_guado

